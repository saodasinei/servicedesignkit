                       </div>
                   </div> -->

                </div>



             </div>
             <!-- content -->
           </div>
            <!-- rightside -->
         </div>
         <!-- list-table -->
       </div>

      

        <div id="confirm" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
          <div class="modal-dialog modal-lg">
           <div class="modal-content">
              <div class="cnt">
               <h3>Technique Name</h3>
                 <p id="ct-name"> </p>

               <h3>Description</h3>
                 <p id="ct-dc"> </p>
               <h3>Instruction</h3>
                 <p id="ct-is"></p>
              <h3>Reference</h3>
                 <p id="ct-rf"></p>
              
              <div class="row">
                   
                   <div class="col-2">
                     <h5>
                       Desigh Phase
                     </h5>
                       <p class="ctattr" id="ct-dp"></p>
                     
                   </div>
                   <div class="col-2">
                     <h5>
                      Time Dependency
                     </h5>
                       <p class="ctattr" id="ct-td"></p>
                     
                   </div>
                   <div class="col-2">
                     <h5>
                      Duration
                     </h5>
                       <p class="ctattr" id="ct-d"></p>
                     
                   </div>
                   <div class="col-2">
                     <h5>
                       User Participation
                     </h5>
                       <p class="ctattr" id="ct-up"></p>
                     
                   </div>
                   <div class="col-2">
                     <h5>
                       Evaluation Type
                     </h5>
                       <p class="ctattr" id="ct-et"></p>
                     
                   </div>
                  </div> 
                       
                       
                  <div class="row">
                    <div class="col-2">
                      <h5>
                       Tag1
                     </h5>
                        <p id="ct-tag0"></p>
                    </div>

                    <div class="col-2">
                      <h5>
                       Tag2
                     </h5>
                        <p id="ct-tag1"></p>
                    </div>
                    <div class="col-2">
                      <h5>
                       Tag3
                     </h5>
                        <p id="ct-tag2"></p>
                    </div>
                    <div class="col-2">
                      <h5>
                       Tag4
                     </h5>
                        <p id="ct-tag3"></p>
                    </div>
                    <div class="col-2">
                      <h5>
                       Tag5
                     </h5>
                        <p id="ct-tag4"></p>
                    </div>
                     
                      
                     
                   </div>

                   <button type="button" class="btn btn-primary"  id="confirm">Confirm</button>
               </div>
            
             

              <!-- cnt -->
             </div>
            </div>
           </div> 

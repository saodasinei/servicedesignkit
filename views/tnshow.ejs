 <!-- Page Content -->
    <div id="selecter">
          <div class="container">
            <a href="/techniques" class="btn btn-secondary" id="backtofilter"><i class="fa fa-fw fa-arrow-left"></i>Back to Filter</a>
            <div class="accordion" id="accordionExample">    
               <a class="btn btn-block"  data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                <div class="attr">
                 <div class="row justify-content-center">
                  <div class="col-sm">
                    <span>
                      Design Phase
                     </span>
                  </div>
                  <div class="col-sm">
                      <span>
                         Duration
                       </span>
                  </div>
                  <div class="col-sm">
                      <span>
                        Time Dependency
                      </span>
                  </div>
                  <div class="col-sm">
                      <span>
                          User Participation
                        </span>
                  </div>
                  <div class="col-sm">
                     <span>
                           Evaluation Type
                       </span>
                  </div>
                 </div>     
               </div>  
              </a>
            </div>
        
          <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
            <div class="row text-center" id="attr-details">
               <div class="col-sm">
                <%=tn_data.tn_dp%>
              </div>
                <div class="col-sm">
                  <%=tn_data.tn_td%>
                </div>
                 <div class="col-sm">
                   <%=tn_data.tn_d%>
                  </div>
                 <div class="col-sm">
                   <%=tn_data.tn_up%>
                 </div>
                 <div class="col-sm">
                    <%=tn_data.tn_et%>        
                 </div>
              </div>
            </div>
          <!-- container -->
        </div>
      <!-- sleecter -->
     </div>


    <div class="container" id="tn-details">

      
      <!-- technique information -->
      <div class="row">
        
        <div class="col-md-2">
          <img class="img-fluid" src="../images/<%=tn_data.tn_img%>" alt="" id="tn-img">
        </div>
        <div class="col-md-8">
          <h1 class=""><%=tn_data.tn_name%></h1>
          <% if (tn_like.length > 0) {%>
           <span class="iflike like">Remove from like list<li name="<%=tn_data.tn_id%>" class="fa fa-fw fa-heart like"></li></span>
          
           <% } else {%>
           <span class="iflike notlike">Add to like list<li name="<%=tn_data.tn_id%>" class="fa fa-fw fa-heart notlike"></li></span>
          
           <% } %>

           <div class="tndetails my-3">Description</div>
            <p>
             <%= tn_data.tn_dc%>
            </p>
          <div class="tndetails my-3">Short Instruction</div>
          <p>
           <%- tn_data.tn_is%>
          </p>
          <div class="tndetails my-3">Reference</div>
          <p>
           <%- tn_data.tn_rf%>
          </p>


        <!-- Related Projects Row -->
       <h3 class="my-4">Comparable</h3>
         <p>The techniques below have the same characteristics.</p>
         <div class="row">
           
           <div class="col-md-3 col-sm-6 mb-4">
             <a href="/techniques/details?tn_id=<%=tn_f[0].tn_id%>">
               <img class="img-fluid" src="../images/<%=tn_f[0].tn_id%>.jpg" alt="">
             </a>
           </div>

           <div class="col-md-3 col-sm-6 mb-4">
               <a href="/techniques/details?tn_id=<%=tn_f[1].tn_id%>">
                <img class="img-fluid" src="../images/<%=tn_f[1].tn_id%>.jpg" alt="">
               </a>
           </div>

           <div class="col-md-3 col-sm-6 mb-4">
               <a href="/techniques/details?tn_id=<%=tn_f[2].tn_id%>">
                 <img class="img-fluid" src="../images/<%=tn_f[2].tn_id%>.jpg" alt="">
               </a>
           </div>

           <div class="col-md-3 col-sm-6 mb-4">
               <a href="/techniques/details?tn_id=<%=tn_f[3].tn_id%>">
                <img class="img-fluid" src="../images/<%=tn_f[3].tn_id%>.jpg" alt="">
               </a>
           </div>
       <!-- /.row -->
       </div>
      
      
      <!-- complementary techniques row -->
        <h3 class="my-4">Complementary</h3>
        <p>The techniques below have different characteristics.</p>
         <div class="row">

           <div class="col-md-3 col-sm-6 mb-4">
               <a href="/techniques/details?tn_id=<%=tn_c[tn_c.length-1].tn_id%>">
                 <img class="img-fluid" src="../images/<%=tn_c[tn_c.length-1].tn_id%>.jpg" alt="">
               </a>
           </div>

           <div class="col-md-3 col-sm-6 mb-4">
                <a href="/techniques/details?tn_id=<%=tn_c[tn_c.length-2].tn_id%>">
                   <img class="img-fluid" src="../images/<%=tn_c[tn_c.length-2].tn_id%>.jpg" alt="">
                </a>
           </div>

           <div class="col-md-3 col-sm-6 mb-4">
                 <a href="/techniques/details?tn_id=<%=tn_c[tn_c.length-3].tn_id%>">
                    <img class="img-fluid" src="../images/<%=tn_c[tn_c.length-3].tn_id%>.jpg" alt="">
                 </a>
           </div>

           <div class="col-md-3 col-sm-6 mb-4">
                 <a href="/techniques/details?tn_id=<%=tn_c[tn_c.length-4].tn_id%>">
                    <img class="img-fluid" src="../images/<%=tn_c[tn_c.length-4].tn_id%>.jpg" alt="">
                 </a>
          </div>
      <!-- row -->
       </div>
       

        <!-- tags -->
        </div>
        <div class="col-md-2">
          <div id="tag-details">Tags</div>

          <% for (i=0; i< tn_tags.length; i++) { %>
          
          <span class="badge badge-secondary"><%= tn_tags[i].tag%></span>

          <% } %>
        </div>
        <!-- technique inforamtion row -->
      </div>
     
     <!-- /.container -->
    </div>
    
     





